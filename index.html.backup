    <!-- 인증 JavaScript -->
    <script>
        // 🎯 회원가입 모달 열기/닫기
        function openRegisterModal() {
            // 소모임 인증 확인
            const isVerified = checkSomoimAuth();
            const somoimNotice = document.getElementById('somoimNotice');
            
            if (somoimNotice) {
                somoimNotice.style.display = isVerified ? 'none' : 'block';
            }
            
            showRegisterModal();
        }
        
        function showRegisterModal() {
            document.getElementById('registerModal').classList.add('active');
        }

        function closeRegisterModal() {
            document.getElementById('registerModal').classList.remove('active');
            document.getElementById('registerForm').reset();
        }

        // 🔑 로그인 모달 열기/닫기
        function showLoginModal() {
            document.getElementById('loginModal').classList.add('active');
        }

        function closeLoginModal() {
            document.getElementById('loginModal').classList.remove('active');
        }

        // 로그인 → 회원가입 전환
        function switchToRegister() {
            closeLoginModal();
            openRegisterModal();
        }

        // 🔑 소모임 인증 확인
        function checkSomoimAuth() {
            const urlParams = new URLSearchParams(window.location.search);
            const somoimRef = urlParams.get('ref');
            const somoimId = urlParams.get('somoim_id');
            
            // 소모임에서 온 경우 세션에 저장
            if (somoimRef === 'somoim' || somoimId) {
                sessionStorage.setItem('somoim_verified', 'true');
                sessionStorage.setItem('somoim_id', somoimId || 'direct');
                sessionStorage.setItem('somoim_timestamp', Date.now());
                
                // URL 파라미터 제거 (깔끔한 URL)
                const cleanUrl = window.location.origin + window.location.pathname;
                window.history.replaceState({}, document.title, cleanUrl);
            }
            
            // 인증 상태 확인 (24시간 유효)
            const verified = sessionStorage.getItem('somoim_verified');
            const timestamp = parseInt(sessionStorage.getItem('somoim_timestamp') || '0');
            const now = Date.now();
            const validDuration = 24 * 60 * 60 * 1000; // 24시간
            
            if (verified === 'true' && (now - timestamp < validDuration)) {
                return true;
            }
            
            return false;
        }

        // 🎯 회원가입 처리
        async function handleRegister(event) {
            event.preventDefault();
            
            // 소모임 인증 확인
            if (!checkSomoimAuth()) {
                const confirmResult = confirm(
                    '⚠️ 소모임 회원 전용 서비스입니다.\n\n' +
                    '소모임에 가입하시겠습니까?\n\n' +
                    '[확인]을 누르면 소모임 페이지로 이동합니다.'
                );
                
                if (confirmResult) {
                    window.open('https://www.somoim.co.kr/8cafe332-cbff-11ef-b613-0a50aa12fbb11', '_blank');
                }
                
                closeRegisterModal();
                return;
            }
            
            const name = document.getElementById('registerName').value.trim();
            const email = document.getElementById('registerEmail').value.trim();
            const password = document.getElementById('registerPassword').value;
            const passwordConfirm = document.getElementById('registerPasswordConfirm').value;
            const birthYear = document.getElementById('registerAge').value.trim();
            const phone = document.getElementById('registerPhone').value.trim();
            
            // 입력 검증
            if (!name || !email || !password || !passwordConfirm || !birthYear || !phone) {
                alert('모든 항목을 입력해주세요.');
                return;
            }

            // 출생년도 유효성 검증
            const currentYear = new Date().getFullYear();
            const birthYearNum = parseInt(birthYear);
            if (birthYearNum < 1900 || birthYearNum > currentYear) {
                alert(`출생년도는 1900년부터 ${currentYear}년 사이여야 합니다.`);
                return;
            }

            // 출생년도로 나이 계산
            const age = currentYear - birthYearNum;

            // 비밀번호 일치 확인
            if (password !== passwordConfirm) {
                alert('비밀번호가 일치하지 않습니다.');
                return;
            }

            // 비밀번호 강도 확인
            if (password.length < 8) {
                alert('비밀번호는 최소 8자 이상이어야 합니다.');
                return;
            }

            // 전화번호 형식 검증
            const phoneRegex = /^[0-9]{2,3}-[0-9]{3,4}-[0-9]{4}$/;
            if (!phoneRegex.test(phone)) {
                alert('전화번호 형식이 올바르지 않습니다.\n예시: 010-1234-5678');
                return;
            }

            // 이메일 중복 확인 (LocalStorage)
            const existingUsers = JSON.parse(localStorage.getItem('all_users') || '[]');
            if (existingUsers.some(u => u.email === email)) {
                alert('이미 사용 중인 이메일입니다.');
                return;
            }

            // 사용자 정보 생성 (소모임 정보 포함)
            const somoimId = sessionStorage.getItem('somoim_id');
            const user = {
                id: Date.now(),
                name: name,
                email: email,
                password: btoa(password), // Base64 인코딩 (실제로는 해시 사용 권장)
                age: parseInt(age),
                birthYear: parseInt(birthYear),
                phone: phone,
                createdAt: new Date().toISOString(),
                hasFullAccess: true,
                somoimVerified: true,
                somoimId: somoimId,
                // 회원 등급 시스템
                memberGrade: 'waitlist',  // 'waitlist'(대기자), 'experimenter'(실험체), 'graduate'(졸업생)
                gradeLabel: '대기자',
                isVIP: false
            };

            // 전체 사용자 목록에 추가
            existingUsers.push(user);
            localStorage.setItem('all_users', JSON.stringify(existingUsers));

            // 현재 사용자로 로그인
            localStorage.setItem('auth_token', `token_${user.id}`);
            localStorage.setItem('user_info', JSON.stringify(user));
            
            // 📊 활동 추적: 회원가입
            trackActivity('registration', { userId: user.id, userName: user.name });
            
            // 모달 닫기 및 UI 업데이트
            closeRegisterModal();
            updateAuthUI();
            
            // 성공 알림 및 오픈 채팅방 안내
            const goToKakao = confirm(
                `🎉 환영합니다, ${name}님!\n\n` +
                `✅ 대기자로 가입되었습니다.\n` +
                `✅ 대기자용 프롬프트를 사용하실 수 있습니다.\n\n` +
                `🔥 VIP 실험체로 업그레이드하시겠습니까?\n\n` +
                `[확인] 클릭 시 오픈 채팅방으로 이동합니다.\n` +
                `오픈 채팅방에서 추가 혜택과 커뮤니티에 참여하세요!`
            );
            
            if (goToKakao) {
                // 새 탭에서 오픈 채팅방 열기
                window.open('https://open.kakao.com/o/gw6HE99g', '_blank');
                // 📊 활동 추적: 카카오톡 입장
                trackActivity('kakao_join', { userId: user.id, userName: user.name });
            }
        }

        // 🔑 로그인 처리
        function handleLogin(event) {
            event.preventDefault();
            
            const email = document.getElementById('loginEmail').value.trim();
            const password = document.getElementById('loginPassword').value;
            const rememberMe = document.getElementById('rememberMe').checked;
            
            // 사용자 찾기
            const existingUsers = JSON.parse(localStorage.getItem('all_users') || '[]');
            const user = existingUsers.find(u => u.email === email);
            
            if (!user) {
                alert('등록되지 않은 이메일입니다.');
                return;
            }
            
            // 비밀번호 확인
            if (atob(user.password) !== password) {
                alert('비밀번호가 올바르지 않습니다.');
                return;
            }
            
            // 로그인 처리
            localStorage.setItem('auth_token', `token_${user.id}`);
            localStorage.setItem('user_info', JSON.stringify(user));
            
            // 마지막 활동 시간 업데이트
            user.lastActivity = Date.now();
            const userIndex = existingUsers.findIndex(u => u.email === email);
            if (userIndex !== -1) {
                existingUsers[userIndex] = user;
                localStorage.setItem('all_users', JSON.stringify(existingUsers));
            }
            
            // 📊 활동 추적: 로그인
            trackActivity('login', { userId: user.id, userName: user.name });
            
            if (rememberMe) {
                localStorage.setItem('remember_me', 'true');
            }
            
            closeLoginModal();
            updateAuthUI();
            
            alert(`환영합니다, ${user.name}님!`);
        }

        // 🔍 비밀번호 찾기 처리
        function handleForgotPassword(event) {
            event.preventDefault();
            
            const phone = document.getElementById('forgotPhone').value.trim();
            const resultDiv = document.getElementById('resetResult');
            
            // 전화번호로 사용자 찾기
            const existingUsers = JSON.parse(localStorage.getItem('all_users') || '[]');
            const user = existingUsers.find(u => u.phone === phone);
            
            if (!user) {
                resultDiv.style.display = 'block';
                resultDiv.style.background = 'rgba(239, 68, 68, 0.1)';
                resultDiv.style.color = '#dc2626';
                resultDiv.style.padding = '15px';
                resultDiv.style.borderRadius = '8px';
                resultDiv.style.border = '1px solid #fca5a5';
                resultDiv.textContent = '⚠️ 등록된 전화번호가 없습니다.';
                return;
            }
            
            // 비밀번호 복구 정보 표시
            resultDiv.style.display = 'block';
            resultDiv.style.background = 'rgba(16, 185, 129, 0.1)';
            resultDiv.style.color = '#059669';
            resultDiv.style.padding = '15px';
            resultDiv.style.borderRadius = '8px';
            resultDiv.style.border = '1px solid #6ee7b7';
            resultDiv.innerHTML = `
                <strong>✅ 계정을 찾았습니다!</strong><br><br>
                <strong>이메일:</strong> ${user.email}<br>
                <strong>이름:</strong> ${user.name}<br><br>
                임시 비밀번호가 등록된 전화번호로 전송되었습니다.<br>
                (데모 버전: 실제로는 SMS 발송)<br><br>
                <button onclick="switchToLogin()" style="background: #059669; color: white; padding: 10px 20px; border: none; border-radius: 8px; cursor: pointer; font-weight: 700;">
                    로그인하러 가기
                </button>
            `;
        }

        // ========================================
        // 📊 성과지표 추적 시스템
        // ========================================
        
        // 성과지표 초기화
        function initializeMetrics() {
            if (!localStorage.getItem('site_metrics')) {
                const initialMetrics = {
                    // 제작 시간 단축 (기본 75%)
                    timeReduction: 75,
                    // 참여율 증가 (기본 400%)
                    engagementIncrease: 400,
                    // 일관성 달성 (기본 95%)
                    consistencyRate: 95,
                    // 활동 데이터
                    activities: {
                        totalPromptViews: 0,      // 프롬프트 조회수
                        totalPromptDownloads: 0,  // 프롬프트 다운로드 수
                        totalPromptRatings: 0,    // 프롬프트 평가 수
                        totalLoginCount: 0,       // 총 로그인 수
                        totalRegistrations: 0,    // 총 회원가입 수
                        kakaoJoins: 0,            // 카카오톡 입장 수
                        activeUsers: 0            // 활성 사용자 수 (7일 기준)
                    },
                    lastUpdated: new Date().toISOString()
                };
                localStorage.setItem('site_metrics', JSON.stringify(initialMetrics));
            }
        }
        
        // 활동 추적 함수
        function trackActivity(activityType, data = {}) {
            const metrics = JSON.parse(localStorage.getItem('site_metrics') || '{}');
            const activities = metrics.activities || {};
            
            // 활동에 따라 수치 업데이트
            switch(activityType) {
                case 'prompt_view':
                    activities.totalPromptViews = (activities.totalPromptViews || 0) + 1;
                    // 참여율 증가 계산 (조회 1회당 +0.5%)
                    metrics.engagementIncrease = Math.min(500, (metrics.engagementIncrease || 400) + 0.5);
                    break;
                    
                case 'prompt_download':
                    activities.totalPromptDownloads = (activities.totalPromptDownloads || 0) + 1;
                    // 제작 시간 단축 개선 (다운로드 1회당 +0.3%)
                    metrics.timeReduction = Math.min(95, (metrics.timeReduction || 75) + 0.3);
                    // 참여율 증가 (다운로드 1회당 +1%)
                    metrics.engagementIncrease = Math.min(500, (metrics.engagementIncrease || 400) + 1);
                    break;
                    
                case 'prompt_rating':
                    activities.totalPromptRatings = (activities.totalPromptRatings || 0) + 1;
                    // 일관성 달성 개선 (평가 1회당 +0.2%)
                    metrics.consistencyRate = Math.min(99, (metrics.consistencyRate || 95) + 0.2);
                    // 참여율 증가 (평가 1회당 +0.8%)
                    metrics.engagementIncrease = Math.min(500, (metrics.engagementIncrease || 400) + 0.8);
                    break;
                    
                case 'login':
                    activities.totalLoginCount = (activities.totalLoginCount || 0) + 1;
                    // 참여율 증가 (로그인 1회당 +0.3%)
                    metrics.engagementIncrease = Math.min(500, (metrics.engagementIncrease || 400) + 0.3);
                    break;
                    
                case 'registration':
                    activities.totalRegistrations = (activities.totalRegistrations || 0) + 1;
                    // 참여율 증가 (가입 1회당 +2%)
                    metrics.engagementIncrease = Math.min(500, (metrics.engagementIncrease || 400) + 2);
                    break;
                    
                case 'kakao_join':
                    activities.kakaoJoins = (activities.kakaoJoins || 0) + 1;
                    // 참여율 증가 (카카오톡 입장 1회당 +1.5%)
                    metrics.engagementIncrease = Math.min(500, (metrics.engagementIncrease || 400) + 1.5);
                    break;
            }
            
            // 활성 사용자 계산 (7일 이내 활동한 사용자)
            updateActiveUsers();
            
            // 업데이트 시간 기록
            metrics.activities = activities;
            metrics.lastUpdated = new Date().toISOString();
            
            // 저장
            localStorage.setItem('site_metrics', JSON.stringify(metrics));
            
            // 화면에 즉시 반영
            updateMetricsDisplay();
        }
        
        // 활성 사용자 계산
        function updateActiveUsers() {
            const metrics = JSON.parse(localStorage.getItem('site_metrics') || '{}');
            const allUsers = JSON.parse(localStorage.getItem('all_users') || '[]');
            const sevenDaysAgo = Date.now() - (7 * 24 * 60 * 60 * 1000);
            
            // 7일 이내 활동한 사용자 수 계산
            const activeCount = allUsers.filter(user => {
                const lastActivity = user.lastActivity || user.createdAt || 0;
                return lastActivity > sevenDaysAgo;
            }).length;
            
            metrics.activities = metrics.activities || {};
            metrics.activities.activeUsers = activeCount;
            
            localStorage.setItem('site_metrics', JSON.stringify(metrics));
        }
        
        // 화면에 성과지표 업데이트
        function updateMetricsDisplay() {
            const metrics = JSON.parse(localStorage.getItem('site_metrics') || '{}');
            
            // 카운트업 애니메이션 없이 즉시 업데이트
            const timeReductionEl = document.querySelector('[data-count="75"]');
            const engagementEl = document.querySelector('[data-count="400"]');
            const consistencyEl = document.querySelector('[data-count="95"]');
            
            if (timeReductionEl) {
                timeReductionEl.textContent = Math.round(metrics.timeReduction || 75);
                timeReductionEl.setAttribute('data-count', Math.round(metrics.timeReduction || 75));
            }
            if (engagementEl) {
                engagementEl.textContent = Math.round(metrics.engagementIncrease || 400);
                engagementEl.setAttribute('data-count', Math.round(metrics.engagementIncrease || 400));
            }
            if (consistencyEl) {
                consistencyEl.textContent = Math.round(metrics.consistencyRate || 95);
                consistencyEl.setAttribute('data-count', Math.round(metrics.consistencyRate || 95));
            }
        }
        
        // 전화번호 포맷팅 함수 (전역)
        function formatPhoneNumber(input) {
            // 숫자만 추출
            let value = input.value.replace(/[^0-9]/g, '');
            let formatted = '';
            
            // 포맷팅
            if (value.length <= 3) {
                formatted = value;
            } else if (value.length <= 7) {
                formatted = value.slice(0, 3) + '-' + value.slice(3);
            } else if (value.length <= 11) {
                formatted = value.slice(0, 3) + '-' + value.slice(3, 7) + '-' + value.slice(7);
            } else {
                // 11자리 초과 시 자르기
                formatted = value.slice(0, 3) + '-' + value.slice(3, 7) + '-' + value.slice(7, 11);
            }
            
            input.value = formatted;
        }

        // 로그인 확인 후 카카오톡 오픈채팅방 입장
        function checkLoginAndOpenKakao() {
            const token = localStorage.getItem('auth_token');
            const user = localStorage.getItem('user_info');
            
            if (!token || !user) {
                // 로그인되지 않은 경우
                const shouldRegister = confirm(
                    '💬 오픈채팅방 입장\n\n' +
                    '⚠️ 회원 전용 서비스입니다.\n\n' +
                    '오픈채팅방은 회원만 입장할 수 있습니다.\n' +
                    '지금 회원가입하시겠습니까?\n\n' +
                    '[확인] 클릭 시 회원가입 페이지로 이동합니다.'
                );
                
                if (shouldRegister) {
                    showRegisterModal();
                }
                return;
            }
            
            // 로그인된 경우 - 오픈채팅방으로 이동
            const userData = JSON.parse(user);
            const goToKakao = confirm(
                `👋 ${userData.name}님, 환영합니다!\n\n` +
                `💬 VIP 실험체 오픈채팅방으로 이동하시겠습니까?\n\n` +
                `✅ 커뮤니티 참여\n` +
                `✅ 1:1 컨설팅\n` +
                `✅ 특별 이벤트 참여\n\n` +
                `[확인] 클릭 시 새 탭에서 오픈채팅방이 열립니다.`
            );
            
            if (goToKakao) {
                window.open('https://open.kakao.com/o/gw6HE99g', '_blank');
                // 📊 활동 추적: 카카오톡 입장
                trackActivity('kakao_join', { userId: userData.id, userName: userData.name });
            }
        }

        // 모달 전환 함수들
        function openForgotPassword() {
            closeLoginModal();
            document.getElementById('forgotPasswordModal').classList.add('active');
        }

        function closeForgotPassword() {
            document.getElementById('forgotPasswordModal').classList.remove('active');
            document.getElementById('forgotPasswordForm').reset();
            document.getElementById('resetResult').style.display = 'none';
        }

        function switchToLogin() {
            closeForgotPassword();
            closeRegisterModal();
            showLoginModal();
        }

        // 🚪 로그아웃 처리
        function handleLogout() {
            if (confirm('정말 로그아웃 하시겠습니까?')) {
                localStorage.removeItem('auth_token');
                localStorage.removeItem('user_info');
                alert('로그아웃되었습니다.');
                updateAuthUI();
            }
        }

        // 🔄 UI 업데이트 (로그인 상태 반영)
        function updateAuthUI() {
            const token = localStorage.getItem('auth_token');
            const userStr = localStorage.getItem('user_info');
            const adminToken = localStorage.getItem('admin_token');
            
            const authButtons = document.getElementById('authButtons');
            const userMenu = document.getElementById('userMenu');
            const userNameDisplay = document.getElementById('userNameDisplay');
            const userDropdownName = document.getElementById('userDropdownName');
            const userDropdownEmail = document.getElementById('userDropdownEmail');
            const adminMenuItems = document.getElementById('adminMenuItems');
            
            if (token && userStr) {
                // 로그인 상태
                const user = JSON.parse(userStr);
                authButtons.style.display = 'none';
                userMenu.style.display = 'block';
                userNameDisplay.textContent = user.name;
                userDropdownName.textContent = user.name;
                userDropdownEmail.textContent = user.email;
                
                // 관리자 메뉴 표시 여부
                if (adminToken) {
                    adminMenuItems.style.display = 'block';
                } else {
                    adminMenuItems.style.display = 'none';
                }
            } else if (adminToken) {
                // 관리자만 로그인 상태
                const adminUser = JSON.parse(localStorage.getItem('admin_user') || '{}');
                authButtons.style.display = 'none';
                userMenu.style.display = 'block';
                userNameDisplay.textContent = adminUser.username || '관리자';
                userDropdownName.textContent = adminUser.username || '관리자';
                userDropdownEmail.textContent = '관리자 계정';
                adminMenuItems.style.display = 'block';
            } else {
                // 비로그인 상태
                authButtons.style.display = 'flex';
                userMenu.style.display = 'none';
            }
        }

        // 사용자 메뉴 드롭다운 토글
        function setupUserMenu() {
            const userMenuBtn = document.getElementById('userMenuBtn');
            const userDropdown = document.getElementById('userDropdown');
            
            if (userMenuBtn) {
                userMenuBtn.addEventListener('click', (e) => {
                    e.stopPropagation();
                    userDropdown.style.display = userDropdown.style.display === 'none' ? 'block' : 'none';
                });
            }
            
            // 외부 클릭 시 닫기
            document.addEventListener('click', () => {
                if (userDropdown) {
                    userDropdown.style.display = 'none';
                }
            });
        }

        // 모달 외부 클릭 시 닫기
        document.getElementById('registerModal').addEventListener('click', (e) => {
            if (e.target.id === 'registerModal') {
                closeRegisterModal();
            }
        });

        document.getElementById('loginModal').addEventListener('click', (e) => {
            if (e.target.id === 'loginModal') {
                closeLoginModal();
            }
        });

        // 페이지 로드 시 초기화
        document.addEventListener('DOMContentLoaded', () => {
            // 성과지표 초기화 및 표시
            initializeMetrics();
            updateMetricsDisplay();
            
            // 소모임 인증 체크
            checkSomoimAuth();
            
            // UI 초기화
            updateAuthUI();
            setupUserMenu();
        });
    </script>

    <!-- Stats Counter Animation -->
    <script>
        function animateCounter(id, target, suffix = '') {
            const element = document.getElementById(id);
            const duration = 2000; // 2 seconds
            const steps = 60;
            const increment = target / steps;
            let current = 0;
            
            const timer = setInterval(() => {
                current += increment;
                if (current >= target) {
                    element.textContent = Math.round(target) + suffix;
                    clearInterval(timer);
                } else {
                    element.textContent = Math.round(current) + suffix;
                }
            }, duration / steps);
        }
        
        // Intersection Observer for stats
        const statsObserver = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting && entry.target.classList.contains('stats-dashboard')) {
                    animateCounter('stat1', 75, '%');
                    animateCounter('stat2', 1200);
                    animateCounter('stat3', 98);
                    animateCounter('stat4', 10);
                    statsObserver.unobserve(entry.target);
                }
            });
        }, { threshold: 0.3 });
        
        // Observe stats section
        document.addEventListener('DOMContentLoaded', () => {
            const statsSection = document.querySelector('.stats-dashboard');
            if (statsSection) {
                statsObserver.observe(statsSection);
            }
        });
    </script>

